<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:WedDevStackController.Styles">

    <Color x:Key="C_Pink">#FE4365</Color>
    <Color x:Key="C_Black">#000000</Color>
    
    <SolidColorBrush x:Key="B_Pink" Color="{StaticResource C_Pink}" />
    <SolidColorBrush x:Key="B_Black" Color="{StaticResource C_Black}" />

</ResourceDictionary>